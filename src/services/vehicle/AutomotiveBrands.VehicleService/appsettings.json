{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SwaggerSetting": {
    "DefinitionName": "Otomotiv API - Arac Servisi",
    "Title": "Vehicle API - Dogus Otomotiv Servis ve Ticaret A.S",
    "Description": "Otomotiv API Projesinin araç işlemlerini içeren servistir.",
    "Version": "v1",
    "TermsOfService": "TermsOfService",
    "ContactName": "LTunes Tribe",
    "ContactEmail": "itunes@dogusgrubu.onmicrosoft.com",
    "ContactUrl": "https://dogusoto.com.tr/iletisim",
    "LicenseName": "Private Licence",
    "LicenseUrl": "https://dogusoto.com.tr/iletisim"
  },
  "ApiVersionSetting": {
    "HeaderName": "x-api-version",
    "MajorVersion": 1,
    "MinorVersion": 0
  },

  "IpRateLimitOptions": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "HttpStatusCode": 429,
    "IpWhitelist": [ "127.0.0.1" ],
    "GeneralRules": [
      {
        "Endpoint": "*:/api/contacts/sendemail",
        "Period": "1m",
        "Limit": 2
      },
      {
        "Endpoint": "*:/api/vehiclestocks/gettyresizesbychassisnumber/*",
        "Period": "1m",
        "Limit": 10
      },
      {
        "Endpoint": "*:/api/appointments/updatepreferences",
        "Period": "1m",
        "Limit": 3
      },
      {
        "Endpoint": "*:/api/orders/addtemporarily",
        "Period": "1m",
        "Limit": 3
      }
    ],
    "QuotaExceededResponse": {
      "Content": "{{ \"succeeded\": false, \"statusMessage\":\"İşleminizi şu anda gerçekleştiremiyoruz. Lütfen bir süre sonra tekrar deneyiniz.\"}}",
      "ContentType": "application/json",
      "StatusCode": 429
    }
  }
}